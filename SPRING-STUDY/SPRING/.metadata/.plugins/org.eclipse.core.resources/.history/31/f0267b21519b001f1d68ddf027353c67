package com.aloha.spring.dto;

public class Board {

	pirvate String title;
	pirvate String writer;
	pirvate String content;
	
	public Board(String title, String writer, String content) {
		this.title = title;
		this.writer = writer;
		this.content = content;
	}

	public pirvate getString() {
		return String;
	}

	public void setString(pirvate string) {
		String = string;
	}

	public pirvate getString() {
		return String;
	}

	public void setString(pirvate string) {
		String = string;
	}

	public pirvate getString() {
		return String;
	}

	public void setString(pirvate string) {
		String = string;
	}
	
	
}
